{
  "name": "intranek",
  "version": "1.1.0",
  "appVersion": "v1.1",
  "description": "Intranek - Intranet Solution (Web + Mobile/Cordova)",
  "env": "dev",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "dev": "meteor run --settings settings-development.json",
    "start:dev": "meteor --settings settings-development.json",
    "start:prod": "meteor --settings settings-production.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:services": "jest imports/service/ --coverage",
    "test:context": "jest imports/context/ --coverage",
    "test:components": "jest imports/ui/components/Fichar/ --coverage",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "start:mobile": "powershell -ExecutionPolicy Bypass -Command \"& { $env:CLEAR_HTTP='1'; & scripts\\cordova-sync.ps1 -Mode prepare; meteor run android --settings mobile-config\\settings-development-mobile.json }\"",
    "build:android": "powershell -ExecutionPolicy Bypass -Command \"& { $env:CLEAR_HTTP='0'; & scripts\\cordova-sync.ps1 -Mode prepare; meteor run android --settings settings-production.json }\""
  },
  "dependencies": {
    "@babel/runtime": "^7.24.1",
    "axios": "^1.7.9",
    "caniuse-lite": "^1.0.30001731",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "file-saver": "^2.0.5",
    "js-cookie": "^3.0.5",
    "localforage": "^1.10.0",
    "lucide-react": "^0.537.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "react": "^19.0.0",
    "react-datepicker": "^8.9.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.0.0",
    "react-easy-crop": "^5.1.0",
    "react-icons": "^5.5.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.22.3",
    "react-select": "^5.10.1",
    "react-timer-hook": "^3.0.7",
    "react-toastify": "^11.0.5",
    "uuid": "^10.0.0",
    "validator": "^13.11.0"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.jsx",
      "server": "server/main.js"
    },
    "testModule": "tests/main.js"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^30.2.0",
    "cordova": "^12.0.0",
    "cordova-android": "^12.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-meteor": "^7.3.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.14"
  }
}
